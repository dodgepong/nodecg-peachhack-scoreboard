{
  "name": "nodecg-peachhack-scoreboard",
  "version": "0.0.0",
  "description": "Graphics overlay package for PeachHack Netrunner broadcasts",
  "homepage": "http://peachhack.com",
  "author": {
    "name": "Ben Torell",
    "email": "dodgepong@gmail.com",
    "url": ""
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "netrunner",
    "overlay",
    "cards",
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "~0.9.9",
    "dashboardPanels": [
      {
